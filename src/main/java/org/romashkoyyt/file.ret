/* -- GUESS NUMBER GAME -- */

var input = toNumber(read("Enter number: "));
var answer = rand(1, 100);

while (true) {
    if (input > answer) {
        print("Number is lower than it.");
    } else if (input < answer) {
        print("Number is greater than it.");
    } else if (input == answer) {
        break;
    }
    input = toNumber(read("Enter number: "));
}

print("You win!");